[fzf]
project = junegunn/fzf-bin
target_filename = fzf
target_platform = linux_amd64
version = 0.17.1

[fzf windows]
project = junegunn/fzf-bin
target_filename = fzf.exe
target_platform = windows_amd64
version = 0.17.1

[ripgrep]
project = BurntSushi/ripgrep
target_filename = rg
target_platform = x86_64-unknown-linux-musl
version = 12.1.1

[ripgrep Windows]
project = BurntSushi/ripgrep
target_filename = rg.exe
target_platform = x86_64-pc-windows-msvc
version = 12.1.1

[starship]
pattern = (?P<binname>[a-zA-Z][a-zA-Z0-9_]+)-(?P<platform>(?:[a-zA-Z0-9_]-?)+)
project = starship/starship
target_filename = starship
target_platform = x86_64-unknown-linux-musl
version = 0.0.0

[starship Windows]
pattern = (?P<binname>[a-zA-Z][a-zA-Z0-9_]+)-(?P<platform>(?:[a-zA-Z0-9_]-?)+)
project = starship/starship
target_filename = starship.exe
target_platform = x86_64-pc-windows-msvc
version = 0.0.0